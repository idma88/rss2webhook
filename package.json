{
  "name": "rss2webhook",
  "version": "2.1.0",
  "description": "Checks for updates in the RSS feed and sends a Discord Webhook when changed",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js --ignore 'feeds.json' --ignore 'cache/hashes.json' --ignore 'cache/hooks.json'"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "discord.js": "^12.3.1",
    "dotenv": "8.2.0",
    "md5": "2.3.0",
    "node-html-parser": "1.2.20",
    "rss-parser": "3.9.0"
  },
  "devDependencies": {
    "nodemon": "1.18.4"
  },
  "keywords": [
    "rss",
    "webhook",
    "monitoring"
  ],
  "author": "Dmitrii Ivanov <idma88@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/idma88/rss2webhook.git"
  },
